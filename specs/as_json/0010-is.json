[

  { "it" : "evaluates arguments if more than one",
    "input" :
    [
      "one", "is", [1],
      "two", "is", [2],
      "2", "is", ["get", ["two"]]
    ],
    "output" : ["my val", "value should ==", [2]]
  },

  { "it" : "saves value with uppercase",
    "input" : [
      "my val", "is", [5],
      "final val", "is", [ "get", ["My Val"] ]
    ],
    "output" : [
      "FINAL VAL", "value should ==", [5]
    ]
  },

  {
    "it" : "fails with Missing_Value if no value is passed",
    "input" : [
      "my val", "is", []
    ],
    "output" : [
      "should raise", ["Missing value:"],
      "message should match", [ "\"is\" []" ]
    ]
  },

  {
    "it" : "fails with Value_Already_Created if value already exists",
    "input" : [
      "my val", "is", [1],
      "mY vAl", "is", [2]
    ],
    "output" : [
      "should raise", ["Value already created:"],
      "message should match", ["my val"]
    ]
  }
]
